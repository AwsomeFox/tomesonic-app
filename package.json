{
  "name": "tomesonic-app",
  "version": "0.12.1-beta",
  "author": "AwsomeFox",
  "scripts": {
    "dev": "nuxt --hostname 0.0.0.0 --port 1337",
    "build": "nuxt build",
    "start": "nuxt --hostname 0.0.0.0 --port 1337",
    "generate": "nuxt generate",
    "sync": "nuxt generate && npx cap sync",
    "cap:sync:android": "nuxt generate && npx cap sync android",
    "icons-android": "cordova-res android --skip-config --copy",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "devlive": "ionic cap run -l --external --port=1337",
    "sync-version": "node -e \"const pkg = require('./package.json'); const fs = require('fs'); const gradlePath = './android/app/build.gradle'; let gradle = fs.readFileSync(gradlePath, 'utf8'); gradle = gradle.replace(/versionName \\\"[^\\\"]*\\\"/g, 'versionName \\\"' + pkg.version + '\\\"'); fs.writeFileSync(gradlePath, gradle); console.log('âœ“ Synced version', pkg.version, 'to android/app/build.gradle');\"",
    "test": "node scripts/test-runner.js",
    "test:raw": "playwright test --project=chromium",
    "test:all": "PLAYWRIGHT_ALL_BROWSERS=1 playwright test",
    "test:headed": "playwright test --project=chromium --headed",
    "test:ui": "playwright test --project=chromium --ui",
    "test:debug": "playwright test --project=chromium --debug",
    "test:report": "playwright show-report",
    "test:install": "npx playwright install chromium",
    "test:install-all": "npx playwright install",
    "test:basic": "playwright test basic-functionality.spec.js --project=chromium"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "^7.1.0",
    "@capacitor-community/volume-buttons": "^7.0.0",
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/clipboard": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/device": "^7.0.2",
    "@capacitor/dialog": "^7.0.2",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/status-bar": "^7.0.3",
    "@nuxtjs/axios": "^5.13.6",
    "@teckel/vue-pdf": "^4.3.5",
    "@webnativellc/capacitor-filesharer": "7.0.4",
    "buffer": "^6.0.3",
    "cordova-plugin-screen-orientation": "^3.0.4",
    "core-js": "^3.45.1",
    "date-fns": "^2.30.0",
    "epubjs": "0.3.93",
    "es6-promise-plugin": "^4.2.2",
    "fast-average-color": "^9.5.0",
    "libarchive.js": "^1.3.0",
    "nuxt": "^2.15.7",
    "socket.io-client": "^4.8.1",
    "v-click-outside": "^3.2.0",
    "vue-toastification": "^1.7.14",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@playwright/test": "^1.55.1",
    "autoprefixer": "^10.4.17",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
